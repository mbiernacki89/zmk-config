/ {
    keymap {
        compatible = "zmk,keymap";
        qwerty {
            bindings = <
            /*╭───────────┬───────────┬───────────┬───────────┬───────────┬───────────╮                           ╭───────────┬───────────┬───────────┬───────────┬───────────┬───────────╮*/
            /*│     §     │     1     │     2     │     3     │     4     │     5     │                           │     6     │     7     │     8     │     9     │     0     │  delete   │*/
                &kp NUBS    &kp N1      &kp N2      &kp N3      &kp N4      &kp N5    /*                         */ &kp N6      &kp N7      &kp N8      &kp N9      &kp N0      &kp DEL
            /*├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤*/
            /*│    esc    │     Q     │     W     │     E     │     R     │     T     │                           │     Y     │     U     │     I     │     O     │     P     │ backspace │*/
                &kp ESC     &kp Q       &kp W       &kp E       &kp R       &kp T     /*                         */ &kp Y       &kp U       &kp I       &kp O       &kp P       &kp BSPC
            /*├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤*/
            /*│    tab    │     A     │     S     │     D     │     F     │     G     ├───────────╮   ╭───────────┤     H     │     J     │     K     │     L     │    '"`    │   enter   │*/
                &kp TAB     LCH(A)      LAH(S)      LGH(D)      LSH(F)      LHH(G)    /* encoder */   /* encoder */ RHH(H)      RSH(J)      RGH(K)       RAH(L)     RCHQ        &kp RET
            /*├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤           │   │           ├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤*/
            /*│ CAPS word │     Z     │     X     │     C     │     V     │     B     │   MUTE    │   │   MUTE    │     N     │     M     │     ,;    │     .:    │     ?!    │           │*/
                &caps_word  &kp Z       &kp X       &kp C       LMH(V)      &kp B       &kp MUTE  /* */ &kp MUTE    &kp N       RMH(M)      &com2       &dot2       &quex       &none
            /*╰───────────┴───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤   ├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┴───────────╯*/
            /*                        │           │           │ navigation│  numbers  │   shift   │   │   space   │  symbols  │    alt    │           │           │                        */
                                        &kp LCTRL   &kp LGUI    &mo NAV     &mo NUM     &kp LSHIFT/* */ &kp SPACE   &mo SYM     &kp LALT    &kp RGUI    &kp RCTRL
            /*                        ╰───────────┴───────────┴───────────┴───────────┤           │   │           ├───────────┴───────────┴───────────┴───────────╯                        */
            /*                                                                        ╰───────────╯   ╰───────────╯                                                                        */
            >;

            sensor-bindings = <
                &inc_dec_kp BRI_DN BRI_UP &inc_dec_kp VOL_DN VOL_UP
            >;
        };
        navigation {
            bindings = <
            /*╭───────────┬───────────┬───────────┬───────────┬───────────┬───────────╮                           ╭───────────┬───────────┬───────────┬───────────┬───────────┬───────────╮*/
            /*│           │           │           │           │           │           │                           │           │ prev song │ play/pause│ next song │           │           │*/
                &none       &kp BRI_DN  &kp BRI_UP  &kp VOL_DN  &kp VOL_UP  &kp MUTE  /*                         */ &none       &kp C_PREV  &kp C_PP    &kp C_NEXT  &none       &lock
            /*├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤*/
            /*│  terminal │ screenshot│ screencut │           │           │           │                           │    <<<    │    vvv    │    ^^^    │    >>>    │           │  delete   │*/
                &term       &scrnshot   &scrncut    &none       &none       &none     /*                         */ &kp HOME    &kp PG_DN   &kp PG_UP   &kp END     &none       &kp DEL
            /*├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤*/
            /*│ terminal2 │    ctrl   │    alt    │    cmd    │   shift   │   hyper   ├───────────╮   ╭───────────┤     <     │     v     │     ^     │     >     │           │           │*/
                &termf      &kp LCTRL   &kp LALT    &kp LGUI    &kp LSHIFT  &kp LHPR  /* encoder */   /* encoder */ &kp LEFT    &kp DOWN    &kp UP      &kp RIGHT   &none       &none
            /*├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤           │   │           ├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤*/
            /*│ CAPS word │           │           │           │    meh    │           │   MUTE    │   │           │           │           │           │           │           │ CAPS word │*/
                &caps_word  &none       &none       &none       &kp LMEH    &none       &kp MUTE  /* */ &term       &none       &none       &none       &none       &none       &caps_word
            /*╰───────────┴───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤   ├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┴───────────╯*/
            /*                        │           │           │[activator]│           │           │   │           │           │           │           │           │                        */
                                        &none       &none       &none       &none       &none     /* */ &trans      &trans      &trans      &trans      &trans
            /*                        ╰───────────┴───────────┴───────────┴───────────┤           │   │           ├───────────┴───────────┴───────────┴───────────╯                        */
            /*                                                                        ╰───────────╯   ╰───────────╯                                                                        */
            >;

            sensor-bindings = <
                &inc_dec_kp BRI_DN BRI_UP &inc_dec_kp VOL_DN VOL_UP
            >;
        };
        number {
            // / * - + = % < > , . ^ ( ) _
            bindings = <
            /*╭───────────┬───────────┬───────────┬───────────┬───────────┬───────────╮                           ╭───────────┬───────────┬───────────┬───────────┬───────────┬───────────╮*/
            /*│           │           │           │           │           │           │                           │           │           │           │           │           │  delete   │*/
                &none       &none       &none       &none       &none       &none     /*                         */ &none       &none       &none       &none       &none       &kp DEL
            /*├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤*/
            /*│    esc    │    F1     │    F2     │    F3     │    F4     │    F5     │                           │    F6     │    F7     │    F8     │    F9     │    F10    │ backspace │*/
                &kp ESC     &kp F1      &kp F2      &kp F3      &kp F4      &kp F5    /*                         */ &kp F6      &kp F7      &kp F8      &kp F9      &kp F10     &kp BSPC
            /*├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤*/
            /*│    tab    │     1     │     2     │     3     │     4     │     5     ├───────────╮   ╭───────────┤     6     │     7     │     8     │     9     │     0     │   enter   │*/
                &kp TAB     LCH(N1)     LAH(N2)     LGH(N3)     LSH(N4)     LHH(N5)   /* encoder */   /* encoder */ RHH(N6)     RSH(N7)     RGH(N8)     RAH(N9)     RCH(N0)     &kp RET
            /*├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤           │   │           ├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤*/
            /*│           │    F12    │    F12    │           │           │           │           │   │           │     /     │     *     │     -     │     +     │     =     │           │*/
                &none       &kp F12     &kp F12     &none       &none       &none       &none     /* */ &none       &kp FSLH    &kp ASTRK   &kp MINUS   &kp PLUS    &kp EQUAL   &none
            /*╰───────────┴───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤   ├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┴───────────╯*/
            /*                        │   reset   │ bootloader│           │[activator]│           │   │           │           │           │           │           │                        */
                                        RESET       BOOT_L      &bt BT_CLR  &none       &none     /* */ &trans      &trans      &trans      &trans      &none
            /*                        ╰───────────┴───────────┴───────────┴───────────┤           │   │           ├───────────┴───────────┴───────────┴───────────╯                        */
            /*                                                                        ╰───────────╯   ╰───────────╯                                                                        */
            >;

            sensor-bindings = <
                &inc_dec_kp BRI_DN BRI_UP &inc_dec_kp VOL_DN VOL_UP
            >;
        };
        symbol {
            bindings = <
            /*  € - + | ^   { } < > `  */
            /*  ~ / * \ %   ( ) @ # "  */
            /*  £ _ = & $   [ ] ; : !  */
            /*╭───────────┬───────────┬───────────┬───────────┬───────────┬───────────╮                           ╭───────────┬───────────┬───────────┬───────────┬───────────┬───────────╮*/
            /*│     §     │     1     │     2     │     3     │     4     │     5     │                           │     6     │     7     │     8     │     9     │     0     │  delete   │*/
                &kp NUBS    &kp N1      &kp N2      &kp N3      &kp N4      &kp N5    /*                         */ &kp N6      &kp N7      &kp N8      &kp N9      &kp N0      &kp DEL
            /*├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤*/
            /*│    esc    │     €     │     -     │     +     │     |     │     ^     │                           │     {     │     }     │     <     │     >     │     `     │ backspace │*/
                &kp ESC     &euro       &kp MINUS   &kp PLUS    &kp PIPE    &kp CARET /*                         */ &kp LBRC    &kp RBRC    &kp LT      &kp GT      &kp GRAVE   &kp BSPC
            /*├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤*/
            /*│    tab    │     ~     │     /     │     *     │     \     │     %     ├───────────╮   ╭───────────┤     (     │     )     │     @     │     #     │     "     │   enter   │*/
                &kp TAB     LCH(TILDE)  LAH(FSLH)   LGH(ASTRK)  LSH(BSLH)   LHH(PRCNT)/* encoder */   /* encoder */ RHH(LPAR)   RSH(RPAR)   RGH(AT)     RAH(HASH)   RCH(DQT)    &kp RET
            /*├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤           │   │           ├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤*/
            /*│           │     £     │     _     │     =     │     &     │     $     │           │   │           │     [     │     ]     │     ;     │     :     │     !     │ CAPS word │*/
                &none       &pound      &kp UNDER   &kp EQUAL   LMH(AMPS)   &kp DLLR    &none     /* */ &none       &kp LBKT    RMH(RBKT)   &kp SEMI    &kp COLON   &kp EXCL    &caps_word
            /*╰───────────┴───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤   ├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┴───────────╯*/
            /*                        │           │           │           │           │           │   │           │[activator]│           │ bootloader│   reset   │                        */
                                        &none       &none       &none       &none       &none     /* */ &none       &none       &bt BT_CLR  BOOT_L      RESET
            /*                        ╰───────────┴───────────┴───────────┴───────────┤           │   │           ├───────────┴───────────┴───────────┴───────────╯                        */
            /*                                                                        ╰───────────╯   ╰───────────╯                                                                        */
            >;

            sensor-bindings = <
                &inc_dec_kp BRI_DN BRI_UP &inc_dec_kp VOL_DN VOL_UP
            >;
        };
        service {
            bindings = <
            /*╭───────────┬───────────┬───────────┬───────────┬───────────┬───────────╮                           ╭───────────┬───────────┬───────────┬───────────┬───────────┬───────────╮*/
            /*│           │           │           │           │           │           │                           │           │           │           │           │           │           │*/
                &none       &none       &none       &none       &none       &none     /*                         */ &none       &none       &none       &none       &none       &none
            /*├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤*/
            /*│           │           │           │           │           │           │                           │           │           │           │           │           │           │*/
                &none       &none       &none       &none       &none       &none     /*                         */ &none       &none       &none       &none       &none       &none
            /*├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤*/
            /*│           │           │           │           │           │           ├───────────╮   ╭───────────┤           │           │           │           │           │           │*/
                &none       &none       &none       &none       &none       &none     /* encoder */   /* encoder */ &none       &none       &none       &none       &none       &none
            /*├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤           │   │           ├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤*/
            /*│           │           │           │           │           │           │           │   │           │           │           │           │           │           │           │*/
                &none       &none       &none       &none       &none       &none       &none     /* */ &none       &none       &none       &none       &none       &none       &none
            /*╰───────────┴───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤   ├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┴───────────╯*/
            /*                        │           │           │           │[activator]│           │   │           │[activator]│           │           │           │                        */
                                        &none       &none       &none       &none       &none     /* */ &none       &none       &none       &none       &none
            /*                        ╰───────────┴───────────┴───────────┴───────────┤           │   │           ├───────────┴───────────┴───────────┴───────────╯                        */
            /*                                                                        ╰───────────╯   ╰───────────╯                                                                        */
            >;

            sensor-bindings = <
                &inc_dec_kp BRI_DN BRI_UP &inc_dec_kp VOL_DN VOL_UP
            >;
            /*╭───────────┬───────────┬───────────┬───────────┬───────────┬───────────╮                           ╭───────────┬───────────┬───────────┬───────────┬───────────┬───────────╮*/
            /*│           │           │           │           │           │           │                           │           │           │           │           │           │           │*/
            /*├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤*/
            /*│           │           │           │           │           │           │                           │           │           │           │           │           │           │*/             
            /*├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤*/
            /*│           │           │           │           │           │           ├───────────╮   ╭───────────┤           │           │           │           │           │           │*/
            /*├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤           │   │           ├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤*/
            /*│           │           │           │           │           │           │           │   │           │           │           │           │           │           │           │*/
            /*╰───────────┴───────────┼───────────┼───────────┼───────────┼───────────┼───────────┤   ├───────────┼───────────┼───────────┼───────────┼───────────┼───────────┴───────────╯*/
            /*                        │           │           │           │           │           │   │           │           │           │           │           │                        */
            /*                        ╰───────────┴───────────┴───────────┴───────────┤           │   │           ├───────────┴───────────┴───────────┴───────────╯                        */
            /*                                                                        ╰───────────╯   ╰───────────╯                                                                        */
            /*                                                                                                                                                                             */
        };
    };
};