/ {
    keymap {
        compatible = "zmk,keymap";
        qwerty {
            bindings = <
            /*╭───────╭───────────┬───────────┬───────────┬───────────┬───────────╮                           ╭───────────┬───────────┬───────────┬───────────┬───────────╮───────╮*/
            /*│       │  Q        │  W        │  E        │  R        │  T        │                           │  Y        │  U        │  I        │  O        │  P        │       │*/
                &none   &kp Q       &kp W       &kp E       &kp R       &kp T     /*                         */ &kp Y       &kp U       &kp I       &kp O       &kp P       &none
            /*├───────├───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┤───────┤*/
            /*│       │  A        │  S        │  D        │  F        │  G        │                           │  H        │  J        │  K        │  L        │  '"`      │       │*/
                &none   LCH(A)      LAH(S)      LGH(D)      LSH(F)      &kp G     /*                         */ &kp H       RSH(J)      RGH(K)       RAH(L)     RCHQ        &none
            /*├───────├───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┤───────┤*/
            /*│       │  Z        │  X        │  C        │  V        │  B        │                           │  N        │  M        │  ,;       │  .:       │  ?!       │       │*/
                &none   &kp Z       &kp X       &kp C       &kp V       &kp B     /*                         */ &kp N       &kp M       &com2        &dot2      &quex       &none
            /*╰───────╰───────────┴───────────┼───────────┼───────────┼───────────┼───────────╮   ╭───────────┼───────────┼───────────┼───────────┼───────────┴───────────╯───────╯*/
            /*                                            │           │           │           │   │           │           │           │                                            */
                                                            &mo NAV     &mo NUM     &kp LSHIFT/* */ &kp SPACE   &mo SYM     &kp LALT
            /*                                            ╰───────────┴───────────┤           │   │           ├───────────┴───────────╯                                            */
            /*                                                                    ╰───────────╯   ╰───────────╯                                                                    */
            >;
        };
        navigation {
            bindings = <
            /*╭───────╭───────────┬───────────┬───────────┬───────────┬───────────╮                           ╭───────────┬───────────┬───────────┬───────────┬───────────╮───────╮*/
            /*│       │           │           │           │           │           │                           │    <<<    │    vvv    │    ^^^    │    >>>    │           │       │*/
                &none   &term       &scrnshot   &scrncut    &none       &none     /*                         */ &kp HOME    &kp PG_DN   &kp PG_UP   &kp END     &kp BSPC    &none
            /*├───────├───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┤───────┤*/
            /*│       │           │           │           │           │           │                           │     <     │     v     │     ^     │     >     │           │       │*/
                &none   &kp LCTRL   &kp LALT    &kp LGUI    &kp LSHIFT  &kp LHPR  /*                         */ &kp LEFT    &kp DOWN    &kp UP      &kp RIGHT   &kp RET     &none
            /*├───────├───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┤───────┤*/
            /*│       │           │           │           │           │           │                           │           │ prev song │ play/pause│ next song │           │       │*/
                &none   &kp BRI_DN  &kp BRI_UP  &kp VOL_DN  &kp VOL_UP  &kp MUTE  /*                         */ &none       &kp C_PREV  &kp C_PP    &kp C_NEXT  &kp DEL     &none
            /*╰───────╰───────────┴───────────┼───────────┼───────────┼───────────┼───────────╮   ╭───────────┼───────────┼───────────┼───────────┼───────────┴───────────╯───────╯*/
            /*                                            │           │           │           │   │           │           │           │                                            */
                                                            &none       &none       &none     /* */ &none       &none       &bt BT_CLR
            /*                                            ╰───────────┴───────────┤           │   │           ├───────────┴───────────╯                                            */
            /*                                                                    ╰───────────╯   ╰───────────╯                                                                    */
            >;
        };
        number {
            bindings = <
            /*╭───────╭───────────┬───────────┬───────────┬───────────┬───────────╮                           ╭───────────┬───────────┬───────────┬───────────┬───────────╮───────╮*/
            /*│       │  F1       │  F2       │  F3       │  F4       │  F5       │                           │  F6       │  F7       │  F8       │  F9       │  F10      │       │*/
                &none   &kp F1      &kp F2      &kp F3      &kp F4      &kp F5    /*                         */ &kp F6      &kp F7      &kp F8      &kp F9      &kp F10     &none
            /*┼───────├───────────├───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┤───────┤*/
            /*│       │  1        │  2        │  3        │  4        │  5        │                           │  6        │  7        │  8        │  9        │  0        │       │*/
                &none   LCH(N1)     LAH(N2)     LGH(N3)     LSH(N4)     LHH(N5)   /*                         */ RHH(N6)     RSH(N7)     RGH(N8)     RAH(N9)     RCH(N0)     &none
            /*┼───────├───────────┼───────────┼───────────┼───────────├───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┤───────┤*/
            /*│       │  F11      │  F12      │           │           │           │                           │           │           │           │           │           │       │*/
                &none   &kp F11     &kp F12     &none       &none       &none     /*                         */ &none       &none       &none       &none       &none       &none
            /*╰───────╰───────────┴───────────┼───────────┼───────────┼───────────┼───────────╮   ╭───────────┼───────────┼───────────┼───────────┼───────────┴───────────╯───────╯*/
            /*                                            │           │           │           │   │           │           │           │                                            */
                                                            &none       &none       &none     /* */ &none       &none       &none
            /*                                            ╰───────────┴───────────┤           │   │           ├───────────┴───────────╯                                            */
            /*                                                                    ╰───────────╯   ╰───────────╯                                                                    */
            >;
        };
        symbol {
            bindings = <
            /*╭───────╭───────────┬───────────┬───────────┬───────────┬───────────╮                           ╭───────────┬───────────┬───────────┬───────────┬───────────╮───────╮*/
            /*│       │  1        │  2        │  3        │  4        │  5        │                           │  6        │  7        │  8        │  9        │  0        │       │*/
                &none   &kp N1      &kp N2      &kp N3      &kp N4      &kp N5    /*                         */ &kp N6      &kp N7      &kp N8      &kp N9      &kp N0      &none
            /*├───────├───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┤───────┤*/
            /*│       │  &        │  <        │  {        │  (        │  [        │                           │  /        │  *        │  -        │  +        │  =        │       │*/
                &none   LCH(AMPS)   LAH(LT)     LGH(LBRC)   LSH(LPAR)   &kp LBKT  /*                         */ &kp FSLH    RSH(ASTRK)  RGH(MINUS)  RAH(PLUS)   RCH(EQUAL)  &none
            /*├───────├───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┤───────┤*/
            /*│       │  |        │  >        │  }        │  )        │  ]        │                           │  \        │  %        │  _        │  ^        │  #        │       │*/
                &none   &kp PIPE    &kp GT      &kp RBRC    &kp RPAR    &kp RBKT  /*                         */ &kp BSLH    &kp PRCNT   &kp UNDER   &kp CARET   &kp HASH    &none
            /*╰───────╰───────────┴───────────┼───────────┼───────────┼───────────┼───────────╮   ╭───────────┼───────────┼───────────┼───────────┼───────────┴───────────╯───────╯*/
            /*                                            │           │           │           │   │           │  ~        │  @        │                                            */
                                                            &bt BT_CLR  &none       &kp LSHIFT/* */ &kp SPACE   &kp TILDE   &kp AT
            /*                                            ╰───────────┴───────────┤           │   │           ├───────────┴───────────╯                                            */
            /*                                                                    ╰───────────╯   ╰───────────╯                                                                    */
            >;
        };
        system {
            bindings = <
            /*╭───────╭───────────┬───────────┬───────────┬───────────┬───────────╮                           ╭───────────┬───────────┬───────────┬───────────┬───────────╮───────╮*/
            /*│       │  F1       │  F2       │  F3       │  F4       │  F5       │                           │  F6       │  F7       │  F8       │  F9       │  F10      │       │*/
                &none   &kp F1      &kp F2      &kp F3      &kp F4      &kp F5    /*                         */ &kp F6      &kp F7      &kp F8      &kp F9      &kp F10     &none
            /*├───────├───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┤───────┤*/
            /*│       │           │           │           │           │  F11      │                           │  F12      │           │           │           │           │       │*/
                &none   &kp LCTRL   &kp LALT    &kp LGUI    &kp LSHIFT  &kp F11   /*                         */ &kp F12     &kp RSHIFT  &kp RGUI    &kp RALT    &kp RCTRL   &none
            /*├───────├───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┤───────┤*/
            /*│       │           │           │           │           │           │                           │           │           │           │           │           │       │*/
                &none   &none       &none       &none       &none       &none     /*                         */ &none       &none       &bt BT_PRV  &bt BT_NXT  &bt BT_CLR  &none
            /*╰───────╰───────────┴───────────┼───────────┼───────────┼───────────┼───────────╮   ╭───────────┼───────────┼───────────┼───────────┼───────────┴───────────╯───────╯*/
            /*                                            │           │           │           │   │           │           │           │                                            */
                                                            &none       &none       &none     /* */ &none       &none       &none
            /*                                            ╰───────────┴───────────┤           │   │           ├───────────┴───────────╯                                            */
            /*                                                                    ╰───────────╯   ╰───────────╯                                                                    */
            >;
        };
    };
};