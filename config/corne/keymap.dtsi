/ {
    keymap {
        compatible = "zmk,keymap";
        qwerty {
            bindings = <
            /*╭───────╭───────────┬───────────┬───────────┬───────────┬───────────╮                           ╭───────────┬───────────┬───────────┬───────────┬───────────╮───────╮*/
            /*│       │     Q     │     W     │     E     │     R     │     T     │                           │     Y     │     U     │     I     │     O     │     P     │       │*/
                &none   &kp Q       &kp W       &kp E       &kp R       &kp T     /*                         */ &kp Y       &kp U       &kp I       &kp O       &kp P       &none
            /*├───────├───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┤───────┤*/
            /*│       │     A     │     S     │     D     │     F     │     G     │                           │     H     │     J     │     K     │     L     │     '     │       │*/
                &none   LCH(A)      LAH(S)      LGH(D)      LSH(F)      LHH(G)    /*                         */ RHH(H)      RSH(J)      RGH(K)      RAH(L)      RCH(SQT)    &none
            /*├───────├───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┤───────┤*/
            /*│       │     Z     │     X     │     C     │     V     │     B     │                           │     N     │     M     │     ,     │     .     │     ?     │       │*/
                &none   &kp Z       &kp X       &kp C       LMH(V)      &kp B     /*──────────╮   ╭──────────*/ &kp N       RMH(M)      &kp COMMA   &kp DOT     &kp QMARK   &none
            /*╰───────╰───────────┴───────────┴───────────┼───────────┼───────────┤           │   │           ├───────────┼───────────┼───────────┴───────────┴───────────╯───────╯*/
            /*                                            │           │           │   shift   │   │   space   │           │    alt    │                                            */
                                                            &mo UTL     &mo LWR     &kp LSHIFT/* */ &kp SPACE   &mo RSE     &kp LALT
            /*                                            ╰───────────┴───────────┤           │   │           ├───────────┴───────────╯                                            */
            /*                                                                    ╰───────────╯   ╰───────────╯                                                                    */
            >;
        };
        lower {
            bindings = <
            /*╭───────╭───────────┬───────────┬───────────┬───────────┬───────────╮                           ╭───────────┬───────────┬───────────┬───────────┬───────────╮───────╮*/
            /*│       │     €     │     -     │     +     │     |     │     ^     │                           │     {     │     }     │     <     │     >     │     `     │       │*/
                &none   &euro       &kp MINUS   &kp PLUS    &kp PIPE    &kp CARET /*                         */ &kp LBRC    &kp RBRC    &kp LT      &kp GT      &kp GRAVE   &none
            /*├───────├───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┤───────┤*/
            /*│       │     ~     │     /     │     *     │     \     │     %     │                           │     (     │     )     │     @     │     #     │     "     │       │*/
                &none   LCH(TILDE)  LAH(FSLH)   LGH(ASTRK)  LSH(BSLH)   LHH(PRCNT)/*                         */ RHH(LPAR)   RSH(RPAR)   RGH(AT)     RAH(HASH)   RCH(DQT)    &none
            /*├───────├───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┤───────┤*/
            /*│       │     £     │     _     │     =     │     &     │     $     │                           │     [     │     ]     │     ;     │     :     │     !     │       │*/
                &none   &pound      &kp UNDER   &kp EQUAL   LMH(AMPS)   &kp DLLR  /*──────────╮   ╭──────────*/ &kp LBKT    RMH(RBKT)   &kp SEMI    &kp COLON   &kp EXCL    &none
            /*╰───────╰───────────┴───────────┴───────────┼───────────┼───────────┤           │   │           ├───────────┼───────────┼───────────┴───────────┴───────────╯───────╯*/
            /*                                            │           │[activator]│           │   │           │           │           │                                            */
                                                            &bt BT_CLR  &none       &none     /* */ &trans      &trans      &trans 
            /*                                            ╰───────────┴───────────┤           │   │           ├───────────┴───────────╯                                            */
            /*                                                                    ╰───────────╯   ╰───────────╯                                                                    */
            >;
        };
        raise {
            bindings = <
            /*╭───────╭───────────┬───────────┬───────────┬───────────┬───────────╮                           ╭───────────┬───────────┬───────────┬───────────┬───────────╮───────╮*/
            /*│       │     1     │     2     │     3     │     4     │     5     │                           │     6     │     7     │     8     │     9     │     0     │       │*/
                &none   &kp N1      &kp N2      &kp N3      &kp N4      &kp N5    /*                         */ &kp N6      &kp N7      &kp N8      &kp N9      &kp N0      &none
            /*├───────├───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┤───────┤*/
            /*│       │    ctrl   │    alt    │    cmd    │   shift   │ CAPS word │                           │     <     │     v     │     ^     │     >     │   enter   │       │*/
                &none   &kp LCTRL   &kp LALT    &kp LGUI    &kp LSHIFT  &caps_word/*                         */ &kp LEFT    &kp DOWN    &kp UP      &kp RIGHT   &kp RET     &none
            /*├───────├───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┤───────┤*/
            /*│       │           │           │           │           │           │                           │ backspace │   enter   │    tab    │    esc    │   delete  │       │*/
                &none   &none       &none       &none       &none       RP        /*──────────╮   ╭──────────*/ &kp BSPC    &kp RET     &kp TAB     &kp ESC     &kp DEL     &none
            /*╰───────╰───────────┴───────────┴───────────┼───────────┼───────────┤           │   │           ├───────────┼───────────┼───────────┴───────────┴───────────╯───────╯*/
            /*                                            │           │           │           │   │           │[activator]│           │                                            */
                                                            &trans      &trans      &trans    /* */ &none       &none       &bt BT_CLR
            /*                                            ╰───────────┴───────────┤           │   │           ├───────────┴───────────╯                                            */
            /*                                                                    ╰───────────╯   ╰───────────╯                                                                    */
            >;
        };
        utils {
            bindings = <
            /*╭───────╭───────────┬───────────┬───────────┬───────────┬───────────╮                           ╭───────────┬───────────┬───────────┬───────────┬───────────╮───────╮*/
            /*│       │           │           │           │           │           │                           │           │           │           │           │           │       │*/
                &none   &kp BRI_DN  &kp BRI_UP  &kp VOL_DN  &kp VOL_UP  &kp MUTE  /*                         */ &none       &kp C_PREV  &kp C_PP    &kp C_NEXT  &lock       &none
            /*├───────├───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┤───────┤*/
            /*│       │    F1     │    F2     │    F3     │    F4     │    F5     │                           │    F6     │    F7     │    F8     │    F9     │    F10    │       │*/
                &none   LCH(F1)     LAH(F2)     LGH(F3)     LSH(F4)     &kp F5    /*                         */ &kp F6      RSH(F7)     RGH(F8)     RAH(F9)     RCH(F10)    &none
            /*├───────├───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┤───────┤*/
            /*│       │           │           │ screenshot│ screencut │    F11    │                           │    F12    │           │           │           │           │       │*/
                &none   &none       &none       &scrnshot   &scrncut    &kp F11   /*──────────╮   ╭──────────*/ &kp F12     &none       &bt BT_PRV  &bt BT_NXT  &bt BT_CLR  &none
            /*╰───────╰───────────┴───────────┴───────────┼───────────┼───────────┤           │   │           ├───────────┼───────────┼───────────┴───────────┴───────────╯───────╯*/
            /*                                            │[activator]│           │           │   │           │           │           │                                            */
                                                            &none       &none       &none     /* */ &none       &none       &none
            /*                                            ╰───────────┴───────────┤           │   │           ├───────────┴───────────╯                                            */
            /*                                                                    ╰───────────╯   ╰───────────╯                                                                    */
            >;
        };
            /*╭───────╭───────────┬───────────┬───────────┬───────────┬───────────╮                           ╭───────────┬───────────┬───────────┬───────────┬───────────╮───────╮*/
            /*│       │           │           │           │           │           │                           │           │           │           │           │           │       │*/
            /*├───────├───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┤───────┤*/
            /*│       │           │           │           │           │           │                           │           │           │           │           │           │       │*/
            /*├───────├───────────┼───────────┼───────────┼───────────┼───────────┤                           ├───────────┼───────────┼───────────┼───────────┼───────────┤───────┤*/
            /*│       │           │           │           │           │           ├───────────╮   ╭───────────┤           │           │           │           │           │       │*/
            /*╰───────┴───────────┴───────────┴───────────┼───────────┼───────────┤           │   │           ├───────────┼───────────┼───────────┴───────────┴───────────╯───────╯*/
            /*                                            │           │           │           │   │           │           │           │                                            */
            /*                                            ╰───────────┴───────────┤           │   │           ├───────────┴───────────╯                                            */
            /*                                                                    ╰───────────╯   ╰───────────╯                                                                    */
    };
};